<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Palmeiras SE</title>
</head>
<body>
  <h1>  <% pessoas.forEach((pessoa) => { %>
        <%= pessoa.nome %>
    <% }) %>
</h1>

<h1>
    <% pessoas.forEach((pessoa) => { %>
        <%= pessoa.sobrenome %>
    <% }) %>

</h1>

<h1>
    <% pessoas.forEach((pessoa) => { %>
        <%= pessoa.idade %>
    <% }) %>

</h1>
<h1>
    <% pessoas.forEach((pessoa) => { %>
        <%= pessoa.altura %>
    <% }) %>
</h1>
    <h2 class="button" onclick="location = '/pessoas/criar'">Cancelar</h2>
</body>
</html>
